#include<iostream>
#include<conio.h>
void main()

int i,int arr[],int j,int temp,int c;
cout<<"Enter array size:";
cin>>c;
for (i=0;i<c;i++)
{
cin>>arr[i]
}
for (i=0;i<c;i++)
{
for(j=0;j<(c-i-1);j++)
{if arr[j]>arr[j+1])
{temp=arr[j];
arr[j]= arr[j+1];
arr[j+1]=temp;}}}
cout<<"Elements successfully sorted in ascending order:\n";
for(i=0;i<c;i++)
{cout<<arr[i]<<"";}